{
  "name": "unicorn",
  "version": "0.0.1",
  "description": "Numenta Unicorn Cross-platform HTM Example Desktop Application",
  "main": "frontend/loader.js",
  "browser": {
    "source-map-support": false
  },
  "engines": {
    "node": "4.1.1",
    "npm": "2.14.4"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "scripts": {
    "build": "UNICORN_TARGET=desktop npm run clean && npm install && npm run prepare && npm run prepare:python && npm run electron:packager",
    "check": "npm install && npm prune && npm outdated -depth 0",
    "clean": "npm run clean:npm && npm run clean:db && npm run clean:docs && npm run clean:webpack",
    "clean:db": "rm -f $HOME/Library/Application\\ Support/unicorn/database/*",
    "clean:docs": "rm -Rf frontend/docs/",
    "clean:npm": "rm -Rf node_modules/",
    "clean:webpack": "rm -Rf frontend/browser/assets/bundle/*",
    "desktop": "UNICORN_TARGET=desktop npm run prepare && npm run electron",
    "desktop:debug": "UNICORN_TARGET=desktop npm run prepare && npm run electron:debug",
    "docs": "esdoc -c frontend/config/ESDoc.json",
    "electron": "electron .",
    "electron:debug": "electron --debug=5858 .",
    "electron:packager": "electron-packager . Unicorn --arch=x64 --icon=frontend/assets/icons/unicorn.icns --ignore=.git --ignore=node_modules/electron-* --overwrite --platform=darwin --prune --version=0.33.3 --sign=$UNICORN_CERT_DEV_APPLE",
    "esfmt": "esformatter --diff 'frontend/**/*.js'",
    "lint": "eslint --ext=js --ext=jsx frontend/ tests/js/",
    "prepare": "npm run lint && npm run prepare:webpack",
    "prepare:python": "python backend/unicorn_backend/freeze_model_runner.py",
    "prepare:webpack": "gulp webpack",
    "node-inspector": "node-inspector",
    "serve": "http-server -p 9999 ./frontend/browser/",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha --opts tests/js/mocha.opts tests/js/integration",
    "test:unit": "mocha --opts tests/js/mocha.opts tests/js/unit",
    "test:pipeline": "npm run test:pipeline:unit && npm run test:pipeline:integration",
    "test:pipeline:integration": "mocha --opts tests/js/mocha.pipeline.opts tests/js/integration",
    "test:pipeline:unit": "mocha --opts tests/js/mocha.pipeline.opts tests/js/unit",
    "web": "UNICORN_TARGET=web npm run prepare && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/numenta/numenta-apps/tree/master/unicorn"
  },
  "keywords": [
    "Unicorn",
    "Numenta",
    "Hierarchical",
    "Temporal",
    "Memory",
    "HTM",
    "Machine",
    "Intelligence",
    "Desktop",
    "Application"
  ],
  "homepage": "http://numenta.com",
  "bugs": "https://github.com/numenta/numenta-apps/issues",
  "author": {
    "name": "Numenta",
    "email": "info@numenta.com",
    "url": "http://numenta.com"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "babel": "5.8.23",
    "bunyan": "1.5.1",
    "csv-streamify": "2.0.0",
    "dygraphs": "1.1.1",
    "fbjs": "0.2.1",
    "fluxible": "1.0.1",
    "fluxible-addons-react": "0.2.0",
    "is-electron-renderer": "2.0.0",
    "js-csp": "0.5.0",
    "json-schema-instantiator": "0.2.2",
    "jsonquery-engine": "1.0.2",
    "jsonschema": "1.0.2",
    "level-queryengine": "3.0.1",
    "level-sublevel": "6.5.2",
    "leveldown": "1.4.1",
    "levelup": "1.2.1",
    "material-ui": "0.13.1",
    "nconf": "0.8.2",
    "react": "0.14.2",
    "react-addons-create-fragment": "0.14.2",
    "react-addons-pure-render-mixin": "0.14.2",
    "react-addons-transition-group": "0.14.2",
    "react-addons-update": "0.14.2",
    "react-dom": "0.14.2",
    "react-tap-event-plugin": "0.2.0",
    "roboto-fontface": "0.4.2"
  },
  "devDependencies": {
    "babel-core": "5.8.23",
    "babel-eslint": "4.1.3",
    "babel-loader": "5.3.2",
    "babel-plugin-typecheck": "1.3.0",
    "css-loader": "0.20.2",
    "electron-debug": "0.2.1",
    "electron-packager": "5.1.0",
    "electron-prebuilt": "0.33.3",
    "esdoc": "0.4.1",
    "esdoc-es7-plugin": "0.0.2",
    "esformatter": "0.7.3",
    "eslint": "1.7.2",
    "eslint-plugin-react": "3.6.3",
    "file-loader": "0.8.4",
    "gulp": "3.9.0",
    "gulp-util": "3.0.6",
    "http-server": "0.8.5",
    "json-loader": "0.5.3",
    "lev": "3.2.3",
    "mocha": "2.3.3",
    "node-inspector": "0.12.3",
    "source-map-support": "0.3.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.6",
    "webpack": "1.12.2",
    "webpack-stream": "2.1.1"
  }
}
