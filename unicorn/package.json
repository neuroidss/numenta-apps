{
  "name": "unicorn",
  "version": "0.0.1",
  "description": "Numenta Unicorn Cross-platform HTM Example Desktop Application",
  "main": "js/main/loader.js",
  "browser": {
    "source-map-support": false
  },
  "engines": {
    "node": "4.1.1",
    "npm": "2.14.4"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "scripts": {
    "build": "UNICORN_TARGET=desktop npm run clean && npm install && npm run prepare && npm run prepare:python && npm run electron:packager",
    "check": "npm install && npm prune && npm outdated -depth 0",
    "clean": "npm run clean:npm && npm run clean:db && npm run clean:docs && npm run clean:build && npm run clean:webpack",
    "clean:build": "rm -Rf dist/ && rm -Rf backend/unicorn_backend/nupic/",
    "clean:db": "rm -f $HOME/Library/Application\\ Support/unicorn/database/*",
    "clean:docs": "rm -Rf js/docs/",
    "clean:npm": "rm -Rf node_modules/",
    "clean:webpack": "rm -Rf js/browser/assets/bundle/*",
    "desktop": "UNICORN_TARGET=desktop npm run prepare && npm run electron",
    "desktop:debug": "UNICORN_TARGET=desktop npm run prepare && npm run electron:debug",
    "docs": "esdoc -c js/config/ESDoc.json",
    "electron": "electron .",
    "electron:debug": "electron --debug=5858 .",
    "electron:packager": "electron-packager . Unicorn --arch=x64 --icon=js/assets/icons/unicorn.icns --ignore=.git --ignore=node_modules/electron-* --overwrite --platform=darwin --prune --version=0.33.3 --sign=$UNICORN_CERT_DEV_APPLE",
    "esfmt": "esformatter --diff 'js/**/*.js'",
    "lint": "eslint --ext=js --ext=jsx js/ tests/js/",
    "prepare": "npm run lint && npm run prepare:webpack",
    "prepare:python": "python py/unicorn_backend/freeze_model_runner.py",
    "prepare:webpack": "gulp webpack",
    "node-inspector": "node-inspector",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha --opts tests/js/mocha.opts tests/js/integration",
    "test:pipeline": "npm run test:pipeline:unit && npm run test:pipeline:integration",
    "test:pipeline:integration": "mocha --opts tests/js/mocha.pipeline.opts tests/js/integration",
    "test:pipeline:unit": "mocha --opts tests/js/mocha.pipeline.opts tests/js/unit",
    "test:unit": "mocha --opts tests/js/mocha.opts tests/js/unit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/numenta/numenta-apps/tree/master/unicorn"
  },
  "keywords": [
    "Unicorn",
    "Numenta",
    "Hierarchical",
    "Temporal",
    "Memory",
    "HTM",
    "Machine",
    "Intelligence",
    "Desktop",
    "Application"
  ],
  "homepage": "http://numenta.com",
  "bugs": "https://github.com/numenta/numenta-apps/issues",
  "author": {
    "name": "Numenta",
    "email": "info@numenta.com",
    "url": "http://numenta.com"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "bunyan": "1.5.1",
    "convert-newline": "0.0.5",
    "csv-streamify": "3.0.3",
    "dygraphs": "1.1.1",
    "fbjs": "0.5.1",
    "fluxible": "1.0.3",
    "fluxible-addons-react": "0.2.2",
    "is-electron-renderer": "2.0.0",
    "js-csp": "0.5.0",
    "json-schema-instantiator": "0.3.0",
    "json2csv-stream": "0.1.3",
    "jsonquery-engine": "1.0.2",
    "jsonschema": "1.0.2",
    "level-queryengine": "3.0.1",
    "level-sublevel": "6.5.2",
    "leveldown": "1.4.3",
    "levelup": "1.3.1",
    "material-ui": "0.13.4",
    "moment": "2.10.6",
    "nconf": "0.8.2",
    "react": "0.14.3",
    "react-addons-create-fragment": "0.14.3",
    "react-addons-pure-render-mixin": "0.14.3",
    "react-addons-transition-group": "0.14.3",
    "react-addons-update": "0.14.3",
    "react-dom": "0.14.3",
    "react-tap-event-plugin": "0.2.1",
    "roboto-fontface": "0.4.2"
  },
  "devDependencies": {
    "babel-core": "6.3.17",
    "babel-eslint": "5.0.0-beta6",
    "babel-loader": "6.2.0",
    "babel-plugin-add-module-exports": "0.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.1",
    "babel-plugin-typecheck": "3.5.1",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-runtime": "6.3.19",
    "css-loader": "0.23.0",
    "electron-debug": "0.5.1",
    "electron-packager": "5.1.1",
    "electron-prebuilt": "0.33.3",
    "escope": "3.3.0",
    "esdoc": "0.4.3",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.11.3",
    "file-loader": "0.8.5",
    "gulp": "3.9.0",
    "gulp-util": "3.0.7",
    "json-loader": "0.5.4",
    "lev": "3.3.1",
    "mocha": "2.3.4",
    "node-inspector": "0.12.5",
    "source-map-support": "0.4.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.12",
    "webpack-stream": "3.1.0"
  }
}
